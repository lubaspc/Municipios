<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Municipios.Vistas.Consultor_ZonasRiesgo">

    <StackLayout>
        <ListView x:Name="ListaZonas">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding IGECEM}" Detail="{Binding Desastre}"></TextCell>
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
        <Button x:Name="BtnPrueba" Text="Prueba" Clicked="BtnPrueba_Clicked"></Button>
    </StackLayout>

</ContentPage>